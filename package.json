{
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "yarn exe scripts/launch-next-js-dev-server.ts",
    "exe": "node --loader ts-node/esm/transpile-only --es-module-specifier-resolution=node --require=./suppress-experimental-warnings.cjs",
    "fix": "npm-run-all --continue-on-error \"fix:*\"",
    "fix:eslint": "next lint --fix --report-unused-disable-directives=error",
    "fix:markdownlint": "markdownlint --fix \"**/*\"",
    "fix:prettier": "prettier --write \"**/*\"",
    "fix:yarn-dedupe": "yarn dedupe",
    "lint": "npm-run-all --continue-on-error \"lint:*\"",
    "lint:eslint": "next lint --report-unused-disable-directives=error",
    "lint:markdownlint": "markdownlint \"**/*\"",
    "lint:prettier": "prettier --check \"**/*\"",
    "lint:tsc": "tsc --project .",
    "lint:yarn-dedupe": "yarn dedupe --check",
    "prepare": "husky install",
    "test": "jest"
  },
  "lint-staged": {
    "**/*": [
      "eslint --fix",
      "markdownlint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@next/eslint-plugin-next": "^12.1.4",
    "axios": "^0.26.1",
    "dotenv-flow": "^3.2.0",
    "lodash": "^4.17.21",
    "luxon": "^2.3.1",
    "next": "^12.1.4",
    "next-remote-watch": "^1.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@kachkaev/eslint-config-react": "^0.4.5",
    "@kachkaev/markdownlint-config": "^0.3.0",
    "@kachkaev/prettier-config": "^1.3.1",
    "@types/dotenv-flow": "^3.2.0",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.4.1",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.181",
    "@types/luxon": "^2.3.1",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "chalk": "^5.0.1",
    "envalid": "^7.3.0",
    "eslint": "^8.12.0",
    "execa": "^6.1.0",
    "fs-extra": "^10.0.1",
    "globby": "^13.1.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "js-yaml": "^4.1.0",
    "lint-staged": "^12.3.7",
    "markdownlint-cli": "^0.31.1",
    "npm-run-all": "^4.1.5",
    "object-hash": "^3.0.0",
    "prettier": "^2.6.2",
    "rm-up": "^1.1.0",
    "sleep-promise": "^9.1.0",
    "sort-keys": "^5.0.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "packageManager": "yarn@3.2.0",
  "engines": {
    "node": ">=16.14.0",
    "yarn": ">=1.22.0"
  }
}
